{
    "view engines": {

        "ejs": {
            "module": "consolidate"
        },

        "dust": {
            "module": "path:./lib/renderer",
            "name": "dust"
        },

        "dustx": {
            "module": "path:./lib/renderer"
        },

        "html": {
            "module": "ejs",
            "name": "ejs",
            "renderer": "renderFile"
        },

        "htmlx": {
            "module": "path:./lib/renderer",
            "name": "dust",
            "rendererFactory": "dustFactory"
        },

        "jade": {
            "module": "jade",
            "renderer": "renderFile"
        }

    },

    "express": {
        "view engine": "html"
    }
}